<!doctype html>
<html lang="en-us">
<head>
  {{#block "head"}}
      <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <title>{{#block "title"}}{{/block}}</title>
      <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootswatch/3.3.6/yeti/bootstrap.min.css" />
      <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" />
      <style type="text/css">{{{css}}}</style>
  {{/block}}
</head>
<body>
<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container-fluid">
      {{#block "header"}}{{/block}}
    </div><!-- /.container-fluid -->
</nav>
<main id="sections" class="container" role="main">
  {{#if basics.summary}}
      <section id="section-summary">
        {{#block "summary"}}{{/block}}
      </section><!-- /#section-summary -->
      <hr class="separator" />
  {{/if}}
  {{#if work.length}}
      <section id="section-experience">
        {{#block "experience"}}{{/block}}
      </section><!-- /#section-experience -->
  {{/if}}
  {{#if skills.length}}
      <section id="section-skills">
        {{#block "skills"}}{{/block}}
      </section><!-- /#section-skills -->
  {{/if}}
  {{#if education.length}}
      <section id="section-education">
        {{#block "education"}}{{/block}}
      </section><!-- /#section-education -->
  {{/if}}
  {{#if awards.length}}
      <section id="section-awards">
        {{#block "awards"}}{{/block}}
      </section><!-- /#section-awards -->
  {{/if}}
  {{#if volunteer.length}}
      <section id="section-volunteer">
        {{#block "volunteer"}}{{/block}}
      </section><!-- /#section-volunteer -->
  {{/if}}
  {{#if publications.length}}
      <section id="section-publications">
        {{#block "publications"}}{{/block}}
      </section><!-- /#section-publications -->
  {{/if}}
  {{#if interests.length}}
      <section id="section-interests">
        {{#block "interests"}}{{/block}}
      </section><!-- /#section-interests -->
  {{/if}}
  {{#if references.length}}
      <section id="section-references">
        {{#block "references"}}{{/block}}
      </section><!-- /#section-references -->
  {{/if}}
    <footer id="footer">
      {{#block "footer"}}{{/block}}
    </footer><!-- /#footer -->
  {{#block "scripts"}}
      <script src="//cdnjs.cloudflare.com/ajax/libs/holder/2.9.1/holder.min.js" type="text/javascript"></script>
      <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.2.1/jquery.min.js" type="text/javascript"></script>
      <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" type="text/javascript"></script>
  {{/block}}
</main><!-- /#sections -->
</body>
</html>
