<!doctype html>
<html lang="en-us">
<head>
  {{#block "head"}}
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <!--<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">-->
      <title>{{#block "title"}}{{/block}}</title>
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.6/yeti/bootstrap.min.css" media="all" />
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" media="all" />
      <link rel="stylesheet" href="style.css" />
  {{/block}}
</head>
<body>
<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      {{#block "header"}}{{/block}}
    </div>
</nav>
<main id="sections" class="container" role="main">
  {{#if basics.summary}}
      <section id="section-summary">
        {{#block "summary"}}{{/block}}
      </section>
      <hr class="separator" />
  {{/if}}
  {{#if work.length}}
      <section id="section-experience">
        {{#block "experience"}}{{/block}}
      </section>
  {{/if}}
  {{#if skills.length}}
      <section id="section-skills">
        {{#block "skills"}}{{/block}}
      </section>
  {{/if}}
  {{#if education.length}}
      <section id="section-education">
        {{#block "education"}}{{/block}}
      </section>
  {{/if}}
  {{#if awards.length}}
      <section id="section-awards">
        {{#block "awards"}}{{/block}}
      </section>
  {{/if}}
  {{#if volunteer.length}}
      <section id="section-volunteer">
        {{#block "volunteer"}}{{/block}}
      </section>
  {{/if}}
  {{#if publications.length}}
      <section id="section-publications">
        {{#block "publications"}}{{/block}}
      </section>
  {{/if}}
  {{#if interests.length}}
      <section id="section-interests">
        {{#block "interests"}}{{/block}}
      </section>
  {{/if}}
  {{#if references.length}}
      <section id="section-references">
        {{#block "references"}}{{/block}}
      </section>
  {{/if}}
    <footer id="footer">
      {{#block "footer"}}{{/block}}
    </footer>
  {{#block "scripts"}}
      <script src="https://cdnjs.cloudflare.com/ajax/libs/holder/2.9.1/holder.min.js" type="text/javascript"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.1/jquery.min.js" type="text/javascript"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" type="text/javascript"></script>
  {{/block}}
</main>
</body>
</html>
